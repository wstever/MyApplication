<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.example.burt.lab">

    <application
        tools:replace="android:icon"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LabActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".TabLayoutActivity"/>
        <!--新增个人周报模块-->
        <activity
            android:name=".weekly_report.WeeklyReportActivity"/>
        <activity
            android:name=".weekly_report.WeeklyReportEditActivity"/>
        <activity
            android:name=".weekly_report.WeeklyReportViewActivity"/>
        <!--项目周报界面-->
        <activity
            android:name=".project_report.ProjectReportActivity"/>
        <activity
            android:name=".project_report.ProjectReportEditActivity"/>
        <activity
            android:name=".project_report.ProjectReportViewActivity"/>
    </application>

</manifest>